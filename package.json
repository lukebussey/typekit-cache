    "test": "echo \"Error: no test specified\" && exit 1",
    "minify": "uglifyjs typekit-cache.js -o typekit-cache.min.js --compress --mangle",
    "prepublish": "npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morris/typekit-cache.git"
  },
